#include <iostream>
using namespace std;
int main () {
    int n, count, rem;
    cin >> n;
    count = 0;
        count += n/100;
        rem = n%100;
        count += rem/20;
        rem %= 20;
        count += rem/10;
        rem %= 10;
        count += rem/5;
        rem %= 5;
        count += rem/1;
    cout << count;
    return 0;
}
