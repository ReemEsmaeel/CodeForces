#include <iostream>
using namespace std;

int main() {
    int n;
    int lucky [] = {4, 7,44, 77, 47,74, 444, 447, 474, 477, 744, 747, 777, 474, 747};
    cin >> n;
    for (int i=0; i<14; i++){
        if (n % lucky[i] == 0){
            cout << "YES";
            return (0);
        }
    }
    cout << "NO";
    return 0;
}
